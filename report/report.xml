<odoo>
    <record id="report_pelanggan_xlsx" model="ir.actions.report">
        <field name="name">Pelanggan Excel</field>
        <field name="model">farhanfutsal.pelanggan</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">farhanfutsal.report_pelanggan_xlsx</field>
